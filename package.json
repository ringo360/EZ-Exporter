{
    "name": "ez-exporter",
    "module": "src/main.ts",
    "type": "module",
    "devDependencies": {
        "@types/bun": "latest"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "scripts": {
        "build-win": "bun build src/main.ts --minify --compile --sourcemap --bytecode --target=bun-windows-x64 --outfile build/run_win",
        "build-linux": "bun build src/main.ts --minify --compile --sourcemap --bytecode --target=bun-linux-x64 --outfile build/run_linux"
    }
}
